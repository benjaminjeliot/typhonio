subroutine test_tio_set_quant_range_f

  use pfunit_mod
  use typhonio
  implicit none

  integer :: ierr
  character(len=*), parameter :: file_name = "tio_set_quant_range.h5"
  integer(kind=TIO_FILEK) :: file_id
  integer(kind=TIO_OBJK) :: state_id
  integer(kind=TIO_OBJK) :: mesh_id
  integer(kind=TIO_OBJK) :: quant_id1, quant_id2, quant_id3, quant_id4
  real(kind=8), dimension(2,2,2) :: qdat

  ierr = TIO_Create_f(file_name, file_id, TIO_ACC_REPLACE_F, "TestCode", "V0.0", "01-01-2000", "Test Title")
  @assertEqual(TIO_SUCCESS_F, ierr)
  ierr = TIO_Create_State_f( file_id, "State1", state_id, 1, 0.0_TIO_TIMEK, "seconds" )
  @assertEqual(TIO_SUCCESS_F, ierr)
  
  ierr = TIO_Create_Mesh_f( file_id, state_id, "QuadMesh", mesh_id,                          &
       &                        TIO_MESH_QUAD_COLINEAR_F, TIO_COORD_CARTESIAN_F, .false., "Group1", 1, &
       &                        TIO_REAL8_F, TIO_REAL8_F, TIO_3D_F, 2, 2, 2, 0, 1,                &
       &                        "mm", "mm", "mm", "x", "y", "z" )
  @assertEqual(TIO_SUCCESS_F, ierr)

  ierr = TIO_Create_Quant_f(file_id, mesh_id, "Quant1", quant_id1, TIO_REAL8_F, &
                  TIO_CENTRE_CELL_F, 0, .false., "node")
  @assertEqual(TIO_SUCCESS_F, ierr)

  ierr = TIO_Create_Quant_f(file_id, mesh_id, "Quant2", quant_id2, TIO_REAL4_F, &
                  TIO_CENTRE_CELL_F, 0, .false., "node")
  @assertEqual(TIO_SUCCESS_F, ierr)

  ierr = TIO_Create_Quant_f(file_id, mesh_id, "Quant3", quant_id3, TIO_INTEGER4_F, &
                  TIO_CENTRE_CELL_F, 0, .false., "node")
  @assertEqual(TIO_SUCCESS_F, ierr)

  ierr = TIO_Set_Quant_Range_f(file_id, quant_id1, TIO_REAL8_F, 0._8, 1._8)
  @assertEqual(TIO_SUCCESS_F, ierr)
  ierr = TIO_Set_Quant_Range_f(file_id, quant_id2, TIO_REAL4_F, -1._4, 1._4)
  @assertEqual(TIO_SUCCESS_F, ierr)
  ierr = TIO_Set_Quant_Range_f(file_id, quant_id3, TIO_INTEGER4_F, 0_4, 100_4)
  @assertEqual(TIO_SUCCESS_F, ierr)

end subroutine test_tio_set_quant_range_f

